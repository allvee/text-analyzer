{
	"env": {
		"browser": true,
		"es2021": true
	},
	"globals": {
		"serviceName": true,
		"isLogEnabled": true,
		"__basedir": true
	},
	"extends": "standard",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", 4],
		"no-unused-vars": 1,
		"no-prototype-builtins": "off",
		"prefer-const": 1,
		"comma-spacing": 1,
		"space-in-parens": 1,
		"padded-blocks": 1,
		"space-before-function-paren": "off",
		"semi": "off",
		"camelcase": [
			"error",
			{
				"allow": []
			}
		]
	}
}
